<Window x:Name="Owner_Financial_Window" x:Class="SeleleTravel.OwnerFinancialWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeleleTravel"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Icon="..\..\images\SeleleLogoBalls.png"
        Closed="Owner_Financial_Window_Closed"
        Title="Owner_Financial_Page" Height="800" Width="1200">
    <Grid RenderTransformOrigin="0.5,0.5" Background="#FF84D8FF">
        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="0.152"/>
                <TranslateTransform/>
            </TransformGroup>
        </Grid.RenderTransform>
        <Label x:Name="label" Content="Entity:" HorizontalAlignment="Left" Margin="21.419,15.587,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbxFinancial_entity" HorizontalAlignment="Left" Margin="79.105,19.587,0,0" VerticalAlignment="Top" Width="169.005">
            <ComboBoxItem Content="Clients" HorizontalAlignment="Left" Width="118"/>
            <ComboBoxItem Content="Services" HorizontalAlignment="Left" Width="118"/>
        </ComboBox>
        <Canvas x:Name="cvsClient" HorizontalAlignment="Left" Height="368.321" Margin="21.419,54.926,0,0" VerticalAlignment="Top" Width="397.153">
            <ListBox x:Name="lbFinancial_clientList" Height="368.321" Width="397.153"/>
        </Canvas>
        <Canvas x:Name="cvsService" HorizontalAlignment="Left" Height="368.321" Margin="21.419,54.926,0,0" VerticalAlignment="Top" Width="397.153" d:IsHidden="True">
            <ListBox x:Name="lbFinancial_servicesList" Height="100" Width="100"/>
        </Canvas>
        <ListBox x:Name="lbFinancial_results1" Background="LightGray" HorizontalAlignment="Left" Height="943.413" VerticalAlignment="Top" Width="1019.207" Margin="462.793,15.587,0,0"/>
        <ListBox x:Name="lbFinancial_results2" Background="LightGray" HorizontalAlignment="Left" Height="939.413" Margin="462.793,19.587,0,0" VerticalAlignment="Top" Width="1019.207" d:IsHidden="True"/>
        <Label x:Name="label1" Content="Time Period:" HorizontalAlignment="Left" Margin="21.419,456.941,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbxFinancial_period" HorizontalAlignment="Left" Margin="108.097,460.941,0,0" VerticalAlignment="Top" Width="310.475">
            <ComboBoxItem Content="Annually" HorizontalAlignment="Left" Width="308.475"/>
            <ComboBoxItem Content="MUltiple Months" HorizontalAlignment="Left" Width="308.475"/>
            <ComboBoxItem Content="Monthly" HorizontalAlignment="Left" Width="308.475"/>
            <ComboBoxItem Content="Weekly" HorizontalAlignment="Left" Width="308.475"/>
        </ComboBox>
        <Button x:Name="btnFinancial_view1" Content="View Summary One" HorizontalAlignment="Left" VerticalAlignment="Top" Width="188.67" Margin="21.419,939.04,0,0"/>
        <Button x:Name="btnFinancial_view2" Content="View Summary Two" HorizontalAlignment="Left" Margin="229.572,939.04,0,0" VerticalAlignment="Top" Width="189" d:IsHidden="True"/>
        <Canvas x:Name="cvsYear" HorizontalAlignment="Left" Height="399.206" Margin="21.419,504.658,0,0" VerticalAlignment="Top" Width="397.153" d:IsHidden="True">
            <Label x:Name="label2" Content="Enter Year:" Width="68.017"/>
            <TextBox x:Name="txbYear" Height="23" Canvas.Left="87.637" TextWrapping="Wrap" Text="" Width="120"/>
        </Canvas>
        <Canvas x:Name="cvsMultipleMonths" HorizontalAlignment="Left" Height="399.206" Margin="21.419,504.658,0,0" VerticalAlignment="Top" Width="423.8" d:IsHidden="True">
            <Label x:Name="label3" Content="From:" Canvas.Top="10"/>
            <Label x:Name="label4" Content="To:" Canvas.Left="220.654" Canvas.Top="10"/>
            <ComboBox x:Name="cbxMultipleMonths_from" Canvas.Left="62.68" Canvas.Top="14" Width="134.005">
                <ComboBoxItem Content="January" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="February" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="March" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="April" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="May" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="June" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="July" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="August" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="September" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="October" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="November" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="December" HorizontalAlignment="Left" Width="132.005"/>
            </ComboBox>
            <ComboBox x:Name="cbxMMultipleMonths_to" Canvas.Left="276.805" Canvas.Top="14" Width="136.995">
                <ComboBoxItem Content="January" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="February" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="March" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="April" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="May" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="June" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="July" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="August" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="September" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="October" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="November" HorizontalAlignment="Left" Width="132.005"/>
                <ComboBoxItem Content="December" HorizontalAlignment="Left" Width="132.005"/>
            </ComboBox>

        </Canvas>
        <Canvas x:Name="cvsMonthly" HorizontalAlignment="Left" Height="399.206" Margin="21.419,504.658,0,0" VerticalAlignment="Top" Width="397.153" d:IsHidden="True">
            <DatePicker x:Name="dpMonthly_from" Canvas.Left="64.666" Canvas.Top="11.96" Width="127.637"/>
            <DatePicker x:Name="dpMonthly_to" Canvas.Left="259.476" Canvas.Top="11.96" Width="137.677"/>
            <Label x:Name="label5" Content="From:" Canvas.Top="10"/>
            <Label x:Name="label6" Content="To:" Canvas.Left="205.649" Canvas.Top="10"/>
        </Canvas>
        <Canvas x:Name="cvsWeekly" HorizontalAlignment="Left" Height="399.206" Margin="21.419,504.658,0,0" VerticalAlignment="Top" Width="397.153" d:IsHidden="True">
            <DatePicker x:Name="dpWeekly_day" Canvas.Left="37.927" Canvas.Top="10" Width="150.349"/>
            <Label x:Name="label7" Content="Day:" Canvas.Top="10"/>
        </Canvas>

    </Grid>
</Window>
