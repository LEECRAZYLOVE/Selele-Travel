        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeleleTravel"
        mc:Ignorable="d"
        Title="Log_In" Height="450" Width="800">
    <Grid Background="#FF526BDE">
        <TextBox x:Name="txbLogIn_staffID" TextWrapping="Wrap" Text="" Margin="300,53,264,343"/>
        <Label x:Name="label1" Content="Password:" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.632,1.231" Margin="217,124,0,0"/>
        <Label x:Name="label" Content="Staff ID:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="217,50,0,0"/>
        <Button x:Name="btnLogIn" Content="LOG IN" Click="btnLogIn_Click" Margin="375,188,342,211"/>
