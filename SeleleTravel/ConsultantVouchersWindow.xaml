<Window x:Class="SeleleTravel.ConsultantVouchersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeleleTravel"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Icon="./images/SeleleLogoBalls.png"
        Title="Consultant_Vouchers_Window" Height="450" Width="800">
    <Grid Background="#FFE4D64D">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Grid.ColumnSpan="2" Content="Order Number:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txbConsultant_Vouchers_inputOrder" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="23" Margin="106.703,12.96,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="250"/>
        <Button x:Name="btnConvsultant_Vouchers_viewOrder" Grid.ColumnSpan="2" Content="View" HorizontalAlignment="Left" Margin="372.703,12.96,0,0" VerticalAlignment="Top" Width="95" Click="BtnConvsultant_Vouchers_viewOrder_Click"/>
        <Button x:Name="btnConsultant_Vouchers_selectOrder" Grid.ColumnSpan="2" Content="Select" HorizontalAlignment="Left" Margin="472.703,12.96,0,0" VerticalAlignment="Top" Width="94"/>
        <TextBox x:Name="txbConsultant_Vouchers_viewOrder" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="357.04" Margin="10,51.96,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="556.703" Opacity="0.6"/>
        <Label x:Name="label1" Grid.ColumnSpan="2" Content="Voucher Number:" HorizontalAlignment="Left" Margin="631.993,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnConsultant_Voucher_addNewVoucher" Grid.ColumnSpan="2" Content="Send Voucher" Margin="585.963,389.04,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="185.904"/>
        <TextBlock x:Name="txbConsultsnt_Vouchers_outputVoucherNumber" Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="585.963,62,0,0" TextWrapping="Wrap" Text="txbOutputGenerateVoucherNumber" VerticalAlignment="Top" Height="17.96" Width="148.817"/>

    </Grid>
</Window>
