        <Button x:Name="btnManager_Invoices_add" Content="Add" HorizontalAlignment="Left" Margin="665.336,4.388,0,0" VerticalAlignment="Top" Width="116.664" RenderTransformOrigin="0.494,0.557"/>
        <ListBox x:Name="ltbManager_Invoices_Orders" HorizontalAlignment="Left" Height="350.652" VerticalAlignment="Top" Width="328.792" Margin="453.208,33.388,0,0"/>
        <Button x:Name="btnManager_requestInvoice" Content="Request Invoices" HorizontalAlignment="Left" Margin="453.208,389.04,0,0" VerticalAlignment="Top" Width="328.792"/>
        <CheckBox x:Name="ckbManager_Invoices_serviceProviderConfirmed" Content="Service Provider Confirmed?" HorizontalAlignment="Left" Margin="269,43,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
