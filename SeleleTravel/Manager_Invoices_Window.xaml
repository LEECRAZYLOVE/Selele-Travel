        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeleleTravel"
        mc:Ignorable="d"
        Title="Manager_Invoices_Window" Height="450" Width="800">
    <Grid>
        <Label x:Name="label" Content="Recieved Invoices" HorizontalAlignment="Left" Margin="10,1.388,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="ltbManager_recievedInvoices" HorizontalAlignment="Left" Height="375.612" Margin="10,33.388,0,0" VerticalAlignment="Top" Width="317"/>
