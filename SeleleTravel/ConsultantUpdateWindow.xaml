                    <Label Content="Number of cars:" Margin="18,151.69,745.247,399.464" Style="{DynamicResource LabelStyle}"/>
                    <TextBox x:Name="txbCarHire_numCars" TextChanged="AmountChanged_WHOLENumber" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Margin="210,197,0,0"/>
