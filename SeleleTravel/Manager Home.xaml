            </TabItem>
            <TabItem Header="Compose Message">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Quote Verfications" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="111" Margin="-1,0,0,0">
