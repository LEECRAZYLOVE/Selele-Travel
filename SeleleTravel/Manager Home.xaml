            </TabItem>
            <TabItem Header="Authorizations" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="96">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Search" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="96">
